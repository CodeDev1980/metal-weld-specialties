<script>
    var $temp = $("<input>");
    var $url = $(location).attr('href');
    $('.urlCopy').on('click', function() {
        $("body").append($temp);
        $temp.val($url).select();
        document.execCommand("copy");
        $temp.remove();
        $("span").text("URL copied!");
    })
</script>